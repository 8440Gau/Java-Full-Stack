package StreamConcepts;

public class Duplicationinword {
public static void main(String[] args) {
	String s ="Great responsibility"; 
	int count;
	boolean flag=true;
	
	for(int i=0;i<s.length();i++) {
		for(int j=i+1;j<s.length();j++) {
			if(i==j) {
				flag= true;
				System.out.println(i);
			}
		}
	}
}
}
